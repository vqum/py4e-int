    <raw format="html" xml:space="preserve">&lt;script type="text/javascript" &gt;

  window.onload = function() {

    a = document.getElementById("fl-next")

    // randomly pick one of two relative urls
    var v = Math.floor(Math.random() * 9) + 1;
    if (v % 2 == 0)
    {
